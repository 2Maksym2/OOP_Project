<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:DealHubWPF.ViewModel"
                    xmlns:view="clr-namespace:DealHubWPF.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ProfileVM}">
        <view:Profile />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RegisterPageVM}">
        <view:RegisterPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginPageVM}">
        <view:LoginPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RegisteredUserPageVM}">
        <view:RegisteredUserPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ChatsVM}">
        <view:Chats />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ReviewsVM}">
        <view:Reviews />
    </DataTemplate>

</ResourceDictionary>